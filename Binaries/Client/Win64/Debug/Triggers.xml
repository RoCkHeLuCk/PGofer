<?xml version="1.0" encoding="utf-8"?>
<Triggers Version="1.0">
  <Link Name="regedit" Enabled="true" ReadOnly="false">
    <FileName Type="string">%SystemRoot%\regedit.exe</FileName>
    <Parameter Type="string"></Parameter>
    <Directory Type="string">C:\Windows\</Directory>
    <State Type="Byte">1</State>
    <SingleInstance Type="Boolean">True</SingleInstance>
    <Priority Type="Byte">2</Priority>
    <RunAdmin Type="Boolean">False</RunAdmin>
    <CaptureMsg Type="Boolean">False</CaptureMsg>
    <ScriptBefor Type="string"></ScriptBefor>
    <ScriptAfter Type="string"></ScriptAfter>
    <CanExecute Type="Boolean">True</CanExecute>
  </Link>
</Triggers>
